        "http-proxy": "^1.13.2",
        "react": "^15.6.1",
        "react-addons-test-utils": "^15.6.0",
        "react-dom": "^15.6.1",
        "react-hot-loader": "^1.3.1",
        "react-redux": "^5.0.5",
        "react-router": "^4.1.1",
        "react-router-redux": "^4.0.8",
        "winston": "^2.1.1",

        "@types/react": "^15.0.38",
        "antd": "^2.12.0",
        "autoprefixer": "^7.1.2",
        "autoprefixer-core": "^6.0.1",
        "ava": "^0.21.0",
