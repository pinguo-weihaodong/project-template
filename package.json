{
    "name": "project-template",
    "version": "0.1.0",
    "description": "Project Template",
    "license": "MIT",
    "engines": {
        "node": "6.10.1"
    },
    "scripts": {
        "prebuild": "npm run clean",
        "build": "cross-env NODE_ENV=development webpack --progress",
        "build-pro": "cross-env NODE_ENV=production webpack --progress",
        "clean": "rimraf dist/ && rimraf public",
        "cover": "nyc npm run test",
        "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
        "dev": "react-scripts-ts start",
        "start": "react-scripts-ts start",
        "lint": "npm run lint-ts && npm run lint-js && npm run stylefmt && npm run lint-css ",
        "lint-css": "stylelint \"./src/**/*.css\"",
        "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\"",
        "lint-js": "eslint \"**/*.js\" --fix --ignore-path .gitignore",
        "stylefmt": "stylefmt -R src/",
        "postinstall": "npm run build",
        "pretest": "npm run lint",
        "test": "tsc && ava --verbose",
        "test:watch": "concurrently \"tsc -w\"  \"ava --verbose --watch\"",
        "ghpage": "cross-env PUBLIC_PATH=gh npm run build && node ./scripts/gh-pages.js"
    },
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "https://github.com/"
    },
    "bugs": {
        "url": "https://github.com/"
    },
    "dependencies": {
        "postcss-modules-local-by-default": "^1.2.0",
        "extract-text-webpack-plugin": "^3.0.0",
        "express": "^4.13.4",
